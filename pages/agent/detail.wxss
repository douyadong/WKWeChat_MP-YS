/*
概要区域
*/
.summary .head-portrait{
  height:120rpx;
  width:120rpx;
}
.summary .agent-name{
  color:#4e4e4e;
  font-size:36rpx;
}
.summary .result{
  color:#737373;
  font-size:24rpx;
}
.summary .company-name{
  color:#737373;
  font-size:28rpx;
}
.summary .tags text{
  background-color:#f5f5f5;
  border-radius:6rpx;
  color:#7c7c7c;
  display:inline-block;
  font-size:20rpx;
  margin:10rpx 10rpx 0 0;
  padding:0 15rpx;
}
.summary .row-hd{  
  width:140rpx;  
}
.summary .row-bd{   
  margin-left:150rpx;  
}
/*
用户评价区域
*/
.scores{
  color:#737373;
  font-size:28rpx;
}
.scores .points{
  color:#4e4e4e;
  font-size:40rpx;
}
.subjects text{
  background-color:#e1f0ff;
  border-radius:8rpx;
  color:#4e4e4e;
  display:inline-block;
  font-size:28rpx;
  margin:0 18rpx 18rpx 0;
  padding:10rpx 24rpx;
}
.subjects .bad{
  background-color:#f5f5f5;
  color:#999;
}
/*
二手房源区域
*/
.source-hd{
  color:#4e4e4e;
  font-size:24rpx;
  line-height:110rpx;
  text-align:center;
}