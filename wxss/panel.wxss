.panel{
  background-color:#fff;
}
.panel .panel-hd{
  border-bottom:1rpx solid #f3f3f3;
  color:#4e4e4e;
  font-size:32rpx;
  margin-left:30rpx;
  line-height:90rpx;
}
.panel .panel-bd{
  padding:30rpx;
}
.panel .panel-bd .row{
  margin:10rpx 0;
}
.panel .row-hd,.panel .row-bd{
  font-size:28rpx;
  line-height:40rpx;  
}
.panel .row-hd{
  color:#4e4e4e;
  float:left;    
}
.panel .row-bd{
  color:#b2b2b2;    
}